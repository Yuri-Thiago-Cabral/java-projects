<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>gerenciadorweb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HelloWoldServlet</servlet-name>
    <servlet-class>br.com.treinamento.servlets.v1.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWoldServlet</servlet-name>
    <url-pattern>/api/v1/servlet/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HelloWoldServlet</servlet-name>
    <url-pattern>/api/v1/servlet/helloWorld</url-pattern>
  </servlet-mapping>
  
</web-app>